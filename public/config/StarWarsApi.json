{
    "appName": "Star Wars",
    "apiType": "RestApi",
    "apiBaseUrl": "https://swapi.dev/api/",
    "mainImage": "/images/StarWars.png",
    "sections": ["people", "planets"],
    "people": {
        "endpoint": "people",
        "apiStructure": { "results": "data|results", "count": "data|count" },
        "pagination": "page-number",
        "perPage": 10,
        "itemIncludedKeys": [
            "birth_year",
            "height",
            "mass",
            "hair_color",
            "skin_color",
            "eye_color",
            "homeworld"
        ],
        "conversions": ["name|title", "gender|desc", "url|id|\\d+"]
    },
    "planets": {
        "endpoint": "planets",
        "apiStructure": { "results": "data|results", "count": "data|count" },
        "pagination": "page-number",
        "perPage": 10,
        "itemIncludedKeys": [
            "rotation_period",
            "orbital_period",
            "diameter",
            "climate",
            "gravity",
            "surface_water",
            "population"
        ],
        "conversions": ["name|title", "terrain|desc", "url|id|\\d+"]
    }
}
