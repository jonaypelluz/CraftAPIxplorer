{
    "appName": "Art Institute of Chicago",
    "apiType": "RestApi",
    "apiBaseUrl": "https://api.artic.edu/api/v1/",
    "mainImage": "/images/ ArtInstituteOfChicago.png",
    "sections": ["artworks"],
    "artworks": {
        "endpoint": "artworks",
        "apiStructure": { "results": "data|data", "count": "data|pagination|total" },
        "pagination": "page-number",
        "perPage": 12,
        "itemIncludedKeys": [
            "artist_title",
            "category_titles",
            "artwork_type_title",
            "api_model",
            "place_of_origin"
        ],
        "conversions": ["title|title", "artist_display|desc", "lqip|image"]
    }
}
