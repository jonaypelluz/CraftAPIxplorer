{
    "appName": "Star Wars",
    "apiType": "Api",
    "apiBaseUrl": "https://swapi.dev/api/",
    "mainImage": "/images/StarWars.jpg",
    "sections": ["people"],
    "people": {
        "endpoint": "people",
        "keys": ["results", "count"],
        "pagination": "page",
        "perPage": 10,
        "itemIncludedKeys": [
            "name",
            "gender",
            "birth_year",
            "height",
            "mass",
            "hair_color",
            "skin_color",
            "eye_color",
            "homeworld"
        ],
        "conversions": ["name|title", "gender|desc", "url|id|\\d+"]
    }
}
