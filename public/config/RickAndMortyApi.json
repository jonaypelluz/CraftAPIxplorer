{
    "appName": "Rick and Morty",
    "apiType": "RestApi",
    "apiBaseUrl": "https://rickandmortyapi.com/api/",
    "mainImage": "/images/RickAndMorty.png",
    "sections": ["characters", "locations"],
    "characters": {
        "endpoint": "character",
        "apiStructure": { "results": "data|results", "count": "data|info|count" },
        "pagination": "page-number",
        "perPage": 20,
        "itemIncludedKeys": ["origin", "gender", "species", "status"],
        "conversions": ["name|title", "species|desc", "image|image"]
    },
    "locations": {
        "endpoint": "location",
        "apiStructure": { "results": "data|results", "count": "data|info|count" },
        "pagination": "page-number",
        "perPage": 20,
        "itemIncludedKeys": ["type", "dimension"],
        "conversions": ["name|title", "type|desc"]
    }
}
