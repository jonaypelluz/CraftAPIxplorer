{
    "appName": "Rick and Morty",
    "apiType": "Api",
    "apiBaseUrl": "https://rickandmortyapi.com/api/",
    "mainImage": "/images/RickAndMorty.png",
    "sections": ["characters", "locations"],
    "characters": {
        "endpoint": "character",
        "keys": ["results", "count"],
        "pagination": "page",
        "perPage": 20,
        "itemIncludedKeys": ["origin", "gender", "species", "status"],
        "conversions": ["name|title", "species|desc"]
    },
    "locations": {
        "endpoint": "location",
        "keys": ["results", "count"],
        "pagination": "page",
        "perPage": 20,
        "itemIncludedKeys": ["name", "type", "dimension"],
        "conversions": ["name|title", "type|desc"]
    }
}
